x = [100,110,120,130,140,150]
y = [(n *5)for n in x]
print(y)

def divisible_by_three(n):
    for num in range(1,n+1):
        if num % 3==0:
            print(num)
divisible_by_three(30)   


# flatten list
z = [[1,2],[3,4],[5,6]]
y=[t for sublist in z for t in sublist]
print(y)

def divisible_by_seven():
    w =[]
    for num in range(100,201):
        if num % 7==0:
            w.append(num)
            print(w)
divisible_by_seven()

def smallest(list_int):
    list_int.sort()
    print(list_int[0])
smallest([10,8,9,6,2,1])


x = range(30)
for y in x:
    if y % 3 == 0 and y % 2 == 0:
        print(f"{y} is divisible by 2 and 3")
    elif y % 3 == 0:
        print(f"{y} is divisible by 3")
    elif y % 2 == 0:
        print(f"{y} is divisible by 2")

temp = 35
if temp > 30:
    print("I'ts warm")
elif temp > 20:
    print("Its nice") 
else:
    print("Done")  
       